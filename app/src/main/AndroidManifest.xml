<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="room.escape.android.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QRScannerActivity" />
        <activity android:name=".BlackSmithActivity">
        </activity>
        <activity android:name=".FinishQuestActivity" />
        <activity android:name=".LapuLapuActivity" />
        <activity
            android:name=".ForgingActivity"
            android:theme="@android:style/Theme.Holo.Dialog.NoActionBar"></activity>
        <activity android:name=".EpilogueActivity"  android:theme="@android:style/Theme.NoTitleBar"></activity>
    </application>

</manifest>
